package bit_manipulation

var(
m16  = 65535;
)
var(
m8  = 16711935;
)
var(
m4  = 252645135;
)
var(
m2  = 858993459;
)
var(
m1  = 1431655765;
)
func bitcount(x int) int {
x = ((x & m1) + ((x >> 1) & m1));
x = ((x & m2) + ((x >> 2) & m2));
x = ((x & m4) + ((x >> 4) & m4));
x = ((x & m8) + ((x >> 8) & m8));
x = ((x & m16) + ((x >> 16) & m16));
return  x;
}
func main()  {
println(bitcount(591750230));
println(bitcount(57521883));
println(bitcount(95869805));
}
